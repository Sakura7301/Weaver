{
    "paths": {
        "memory_dir": "ai_memory",
        "memory_db": "ai_memory/memory.db",
        "memory_file": "ai_memory/MEMORY.md",
        "daily_memory_dir": "ai_memory/daily",
        "last_merge_file": "ai_memory/.last_merge",
        "log_dir": "log/"
    },
    "api": {
        "searxng_url": "searxng_url",
        "api_key": "api_key",
        "base_url": "base_url"
    },
    "models": {
        "judge_model": "glm-4-plus",
        "answer_model": "deepseek-chat",
        "embedding_model": "embedding-3"
    },
    "features": {
        "max_fetch": 2,
        "timezone": "Asia/Shanghai",
        "max_history": 10
    },
    "memory": {
        "merge_interval_days": 1,
        "auto_merge_on_startup": true,
        "merge_similarity_threshold": 0.3
    },
    "logging": {
    "log_level": "INFO",
    "log_file": "logs/weaver.log",
    "log_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "log_date_format": "%Y-%m-%d %H:%M:%S",
    "max_log_size": 10485760,
    "backup_count": 5,
    "enable_console": true,
    "enable_file": true
  }
}